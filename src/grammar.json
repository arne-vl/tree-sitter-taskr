{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "taskr",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_definition"
      }
    },
    "_definition": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_env_definition"
        },
        {
          "type": "SYMBOL",
          "name": "task"
        }
      ]
    },
    "_env_definition": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "env"
        },
        {
          "type": "SYMBOL",
          "name": "default_env"
        }
      ]
    },
    "kw_env": {
      "type": "STRING",
      "value": "env"
    },
    "env": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "kw_env"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "SYMBOL",
          "name": "_colon"
        },
        {
          "type": "SYMBOL",
          "name": "file_definition"
        }
      ]
    },
    "kw_default_env": {
      "type": "STRING",
      "value": "default env"
    },
    "default_env": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "kw_default_env"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "SYMBOL",
          "name": "_colon"
        },
        {
          "type": "SYMBOL",
          "name": "file_definition"
        }
      ]
    },
    "file_key": {
      "type": "STRING",
      "value": "file"
    },
    "filename": {
      "type": "PATTERN",
      "value": "[\\.a-z]+"
    },
    "file_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_indent"
        },
        {
          "type": "SYMBOL",
          "name": "file_key"
        },
        {
          "type": "SYMBOL",
          "name": "_space"
        },
        {
          "type": "SYMBOL",
          "name": "_equals"
        },
        {
          "type": "SYMBOL",
          "name": "_space"
        },
        {
          "type": "SYMBOL",
          "name": "filename"
        }
      ]
    },
    "kw_task": {
      "type": "STRING",
      "value": "task"
    },
    "task": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "kw_task"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "SYMBOL",
          "name": "_colon"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "run_definition"
              },
              {
                "type": "SYMBOL",
                "name": "desc_definition"
              },
              {
                "type": "SYMBOL",
                "name": "needs_definition"
              },
              {
                "type": "SYMBOL",
                "name": "alias_definition"
              }
            ]
          }
        }
      ]
    },
    "command": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": 1,
        "content": {
          "type": "PATTERN",
          "value": "[^\\n]+"
        }
      }
    },
    "run_key": {
      "type": "STRING",
      "value": "run"
    },
    "run_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_indent"
        },
        {
          "type": "SYMBOL",
          "name": "run_key"
        },
        {
          "type": "SYMBOL",
          "name": "_space"
        },
        {
          "type": "SYMBOL",
          "name": "_equals"
        },
        {
          "type": "SYMBOL",
          "name": "_space"
        },
        {
          "type": "SYMBOL",
          "name": "command"
        }
      ]
    },
    "desc_key": {
      "type": "STRING",
      "value": "desc"
    },
    "text": {
      "type": "PATTERN",
      "value": ".+"
    },
    "desc_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_indent"
        },
        {
          "type": "SYMBOL",
          "name": "desc_key"
        },
        {
          "type": "SYMBOL",
          "name": "_space"
        },
        {
          "type": "SYMBOL",
          "name": "_equals"
        },
        {
          "type": "SYMBOL",
          "name": "_space"
        },
        {
          "type": "SYMBOL",
          "name": "text"
        }
      ]
    },
    "alias_key": {
      "type": "STRING",
      "value": "alias"
    },
    "alias_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_indent"
        },
        {
          "type": "SYMBOL",
          "name": "alias_key"
        },
        {
          "type": "SYMBOL",
          "name": "_space"
        },
        {
          "type": "SYMBOL",
          "name": "_equals"
        },
        {
          "type": "SYMBOL",
          "name": "_space"
        },
        {
          "type": "SYMBOL",
          "name": "list"
        }
      ]
    },
    "needs_key": {
      "type": "STRING",
      "value": "needs"
    },
    "needs_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_indent"
        },
        {
          "type": "SYMBOL",
          "name": "needs_key"
        },
        {
          "type": "SYMBOL",
          "name": "_space"
        },
        {
          "type": "SYMBOL",
          "name": "_equals"
        },
        {
          "type": "SYMBOL",
          "name": "_space"
        },
        {
          "type": "SYMBOL",
          "name": "list"
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-z]+"
    },
    "_colon": {
      "type": "STRING",
      "value": ":"
    },
    "_equals": {
      "type": "STRING",
      "value": "="
    },
    "_indent": {
      "type": "STRING",
      "value": "  "
    },
    "_space": {
      "type": "STRING",
      "value": " "
    },
    "list": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "SYMBOL",
                "name": "identifier"
              }
            ]
          }
        }
      ]
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": 0,
        "content": {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "//"
            },
            {
              "type": "PATTERN",
              "value": ".*"
            }
          ]
        }
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}