{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "taskr",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_definition"
      }
    },
    "_definition": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "env_definition"
        },
        {
          "type": "SYMBOL",
          "name": "task_definition"
        }
      ]
    },
    "task_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "kw_task"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "required_task_attribute"
              },
              {
                "type": "SYMBOL",
                "name": "optional_task_attribute"
              }
            ]
          }
        }
      ]
    },
    "required_task_attribute": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "  "
        },
        {
          "type": "SYMBOL",
          "name": "required_task_key"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "STRING",
          "value": " "
        },
        {
          "type": "SYMBOL",
          "name": "anything"
        }
      ]
    },
    "optional_task_attribute": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "  "
        },
        {
          "type": "SYMBOL",
          "name": "optional_task_key"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "STRING",
          "value": " "
        },
        {
          "type": "SYMBOL",
          "name": "anything"
        }
      ]
    },
    "list": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "SYMBOL",
                "name": "identifier"
              }
            ]
          }
        }
      ]
    },
    "env_definition": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "env"
        },
        {
          "type": "SYMBOL",
          "name": "default_env"
        }
      ]
    },
    "env": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "kw_env"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "SYMBOL",
          "name": "file_definition"
        }
      ]
    },
    "default_env": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "kw_default_env"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "SYMBOL",
          "name": "file_definition"
        }
      ]
    },
    "file_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "  "
        },
        {
          "type": "SYMBOL",
          "name": "required_env_key"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "STRING",
          "value": " "
        },
        {
          "type": "SYMBOL",
          "name": "filename"
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-z]+"
    },
    "filename": {
      "type": "PATTERN",
      "value": "[\\.a-z]+"
    },
    "anything": {
      "type": "PATTERN",
      "value": ".+"
    },
    "kw_task": {
      "type": "PATTERN",
      "value": "task"
    },
    "kw_env": {
      "type": "PATTERN",
      "value": "env"
    },
    "kw_default_env": {
      "type": "PATTERN",
      "value": "default env"
    },
    "required_task_key": {
      "type": "PATTERN",
      "value": "run"
    },
    "optional_task_key": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "desc"
        },
        {
          "type": "STRING",
          "value": "needs"
        },
        {
          "type": "STRING",
          "value": "alias"
        }
      ]
    },
    "required_env_key": {
      "type": "PATTERN",
      "value": "file"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}