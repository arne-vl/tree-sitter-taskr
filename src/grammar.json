{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "taskr",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_definition"
      }
    },
    "_definition": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "env_definition"
        },
        {
          "type": "SYMBOL",
          "name": "task_definition"
        }
      ]
    },
    "task_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "task"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "task_attribute"
          }
        }
      ]
    },
    "task_attribute": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "run"
        },
        {
          "type": "SYMBOL",
          "name": "desc"
        },
        {
          "type": "SYMBOL",
          "name": "alias"
        },
        {
          "type": "SYMBOL",
          "name": "needs"
        }
      ]
    },
    "run": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "  "
        },
        {
          "type": "STRING",
          "value": "run"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "anything"
        }
      ]
    },
    "desc": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "  "
        },
        {
          "type": "STRING",
          "value": "desc"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "anything"
        }
      ]
    },
    "alias": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "  "
        },
        {
          "type": "STRING",
          "value": "alias"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "list"
        }
      ]
    },
    "needs": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "  "
        },
        {
          "type": "STRING",
          "value": "needs"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "list"
        }
      ]
    },
    "list": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "SYMBOL",
                "name": "identifier"
              }
            ]
          }
        }
      ]
    },
    "env_definition": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "env"
        },
        {
          "type": "SYMBOL",
          "name": "default_env"
        }
      ]
    },
    "env": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "env"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "SYMBOL",
          "name": "file_definition"
        }
      ]
    },
    "default_env": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "default env"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "SYMBOL",
          "name": "file_definition"
        }
      ]
    },
    "file_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "  "
        },
        {
          "type": "STRING",
          "value": "file"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "filename"
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-z]+"
    },
    "filename": {
      "type": "PATTERN",
      "value": "[\\.a-z]+"
    },
    "anything": {
      "type": "PATTERN",
      "value": ".+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}